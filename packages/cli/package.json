{
  "name": "rabbithook",
  "type": "module",
  "version": "0.1.1",
  "description": "Add hooks to your apps.",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucamqf/rabbithook.git",
    "directory": "packages/cli"
  },
  "author": {
    "name": "lucamqf"
  },
  "keywords": [
    "react",
    "hooks"
  ],
  "exports": "./dist/index.js",
  "bin": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "start": "node dist/index.js",
    "pub:beta": "pnpm build && pnpm publish --no-git-checks --access public --tag beta",
    "pub:next": "pnpm build && pnpm publish --no-git-checks --access public --tag next",
    "pub:release": "pnpm build && pnpm publish --access public"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "consola": "^3.2.3",
    "ofetch": "^1.3.4",
    "ora": "^8.0.1",
    "prompts": "^2.4.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/prompts": "^2.4.9",
    "pathe": "^1.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  }
}